@model IEnumerable<E_Administration.Models.Floor>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CreatedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FloorName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedAt)
            </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FloorName)
                </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FloorId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FloorId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FloorId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
